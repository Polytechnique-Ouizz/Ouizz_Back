<p id="notice"><%= notice %></p>

<h1>Ouizzusers</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th>Email</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ouizzusers.each do |ouizzuser| %>
      <tr>
        <td><%= ouizzuser.username %></td>
        <td><%= ouizzuser.password %></td>
        <td><%= ouizzuser.email %></td>
        <td><%= ouizzuser.phone %></td>
        <td><%= link_to 'Show', ouizzuser %></td>
        <td><%= link_to 'Edit', edit_ouizzuser_path(ouizzuser) %></td>
        <td><%= link_to 'Destroy', ouizzuser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ouizzuser', new_ouizzuser_path %>
